{
    "plugins": ["jest"],
    "env": {
        "es2020": true,
        "jest/globals": true
    },
    "extends": [
        "plugin:jest/recommended",
        "plugin:jest/style"
    ],
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "eqeqeq": ["error"],
        "indent": ["error"]
    }
}
